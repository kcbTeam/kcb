<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- FloatingEditText Attribute -->
    <declare-styleable name="FloatingEditText">
        <attr name="floating_edit_text_hint_scale" format="float" />
        <attr name="floating_edit_text_color" format="color" />
        <attr name="floating_edit_text_highlighted_color" format="color" />
        <attr name="floating_edit_text_error_color" format="color" />
        <attr name="floating_edit_text_underline_height" format="dimension" />
        <attr name="floating_edit_text_underline_highlighted_height" format="dimension" />
    </declare-styleable>

    <!-- PaperButton Attribute -->
    <declare-styleable name="PaperButton">
        <attr name="paper_text" format="string" />
        <attr name="paper_text_size" format="dimension" />
        <attr name="paper_text_color" format="color" />
        <attr name="paper_font" format="string" />
        <attr name="paper_color" format="color" />
        <attr name="paper_corner_radius" format="dimension" />
        <attr name="paper_shadow_radius" format="float" />
        <attr name="paper_shadow_offset_x" format="float" />
        <attr name="paper_shadow_offset_y" format="float" />
        <attr name="paper_shadow_color" format="color" />
    </declare-styleable>

</resources>