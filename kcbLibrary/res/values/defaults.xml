    <color name="paper_text_color">#60000000</color>
    <color name="paper_button_color">#ffffffff</color>
    <color name="paper_button_shadow_color">#000000</color>

    <dimen name="paper_button_corner_radius">2dp</dimen>
    <dimen name="paper_padding">8dp</dimen>

    <!-- FloatingEditText Default -->
    <color name="floating_edit_text_color">#bdbdbd</color>
    <color name="floating_edit_text_highlighted_color">#427fed</color>
